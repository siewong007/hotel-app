{
  "$schema": "https://schemas.tauri.app/2/capabilities",
  "identifier": "default",
  "description": "Default capabilities for the Hotel Management System",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:default",
    "shell:allow-open",
    "shell:allow-spawn",
    "shell:allow-execute",
    "shell:allow-kill",
    "shell:allow-stdin-write"
  ]
}
