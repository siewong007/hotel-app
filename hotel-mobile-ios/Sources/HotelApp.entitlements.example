<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Associated Domains for Passkey Authentication -->
    <!-- This enables webcredentials association with your domain -->
    <key>com.apple.developer.associated-domains</key>
    <array>
        <string>webcredentials:hotelapp.com</string>
        <string>applinks:hotelapp.com</string>
    </array>
    
    <!-- Keychain Sharing (optional, if needed for data sharing between apps) -->
    <key>keychain-access-groups</key>
    <array>
        <string>$(AppIdentifierPrefix)com.yourcompany.hotelapp</string>
    </array>
</dict>
</plist>
